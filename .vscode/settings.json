{
  "angular-schematics.schematicsDefaultOptions": {
    "angular-*": {
      "style": "scss",
      "inlineStyle": true,
      "skipStyle": true,
      "externalTemplate": true,
      "selectorPrefix": "oa"
    },
    "angular-component": {
      "ngOnInit": true,
      "skipDisplayBlock": true
    },
    "angular-page": {
      "skipNgOnInit": false
    }
  },
  "i18n-ally.localesPaths": ["src/assets/i18n"],
  "i18n-ally.keystyle": "nested"
}
